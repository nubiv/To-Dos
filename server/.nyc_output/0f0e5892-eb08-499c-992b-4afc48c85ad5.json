AAC;AACL,CAAC,CAAC,CAAC","sourcesContent":["import { describe, it, beforeEach } from 'mocha';\nimport chai, { expect } from 'chai';\nimport { createSandbox } from 'sinon';\nimport sinonChai from 'sinon-chai';\nimport rewire from 'rewire';\nimport { sequelize } from '../models';\n\nchai.use(sinonChai);\n\nconst sandbox = createSandbox();\n\nlet taskController = rewire('../controllers/task.controller');\n\ndescribe('Test /task.controller', () => {\n  let sampleTaskVal: any;\n  let findOneStub: any;\n\n  beforeEach('beforeEach', () => {\n    sampleTaskVal = {\n      content: 'test',\n      status: 'TO DO',\n      userId: '123'\n    };\n\n    findOneStub = sandbox\n      .stub(sequelize.models.Task, 'findAll')\n      .resolves([sampleTaskVal]);\n  });\n\n  afterEach(() => {\n    taskController = rewire('../controllers/task.controller');\n    sandbox.restore();\n  });\n\n  describe('getAllTasks', () => {\n    it('should return expected task list', async () => {\n      const req: any = {\n        params: {\n          userId: '123'\n        }\n      };\n\n      taskController\n        .getAllTasks(req)\n        .then((taskList: any) => {\n          expect(taskList).to.equal([sampleTaskVal]);\n        })\n        .catch((err: any) => {\n          throw new Error('Testing failed.');\n        });\n    });\n\n    it('should return error when called without an userId', async () => {\n      const req: any = {\n        params: {}\n      };\n\n      taskController\n        .getAllTasks(req)\n        .then((taskList: any) => {\n          throw new Error('Testing failed.');\n        })\n        .catch((err: any) => {\n          expect(err).to.be.instanceOf(Error);\n        });\n    });\n  });\n\n  describe('createTask', () => {\n    it('should send an error msg when content is missing', async () => {\n      const req: any = {\n        params: {},\n        body: sampleTaskVal\n      };\n\n      taskController\n        .createNewTask(req)\n        .then((task: any) => {\n          throw new Error('Testing failed');\n        })\n        .catch((err: any) => {\n          expect(err).to.be.instanceOf(Error);\n        });\n    });\n\n    it('should return an expected task', async () => {\n      const req: any = {\n        params: {\n          userId: '123'\n        },\n        body: sampleTaskVal\n      };\n\n      taskController\n        .createNewTask(req)\n        .then((task: any) => {\n          expect(task).to.be.instanceOf(sampleTaskVal);\n        })\n        .catch((err: any) => {\n          throw new Error('Testing failed');\n        });\n    });\n  });\n});\n"]},"_coverageSchema":"1a1c01bbd47fc00a2c39e90264f33305004495a9","hash":"dd4d3c8d33a03fc05953e8af907add7e57d1e948","contentHash":"991a3aab74318359d5c4ad5c236359d263e6d16c579803e7fec218990eff5963"},"/Users/horus/dev/to-dos/server/src/models/index.ts":{"path":"/Users/horus/dev/to-dos/server/src/models/index.ts","statementMap":{"0":{"start":{"line":2,"column":0},"end":{"line":2,"column":62}},"1":{"start":{"line":3,"column":0},"end":{"line":3,"column":27}},"2":{"start":{"line":4,"column":20},"end":{"line":4,"column":50}},"3":{"start":{"line":5,"column":20},"end":{"line":5,"column":40}},"4":{"start":{"line":6,"column":0},"end":{"line":16,"column":3}}},"fnMap":{},"branchMap":{},"s":{"0":1,"1":1,"2":1,"3":1,"4":1},"f":{},"b":{},"inputSourceMap":{"version":3,"file":"/Users/horus/dev/to-dos/server/src/models/index.ts","sources":["/Users/horus/dev/to-dos/server/src/models/index.ts"],"names":[],"mappings":";;;AAAA,mDAA+C;AAC/C,yCAAsC;AAEzB,QAAA,SAAS,GAAG,IAAI,qBAAS,CAAC,oBAAQ,CAAC,QAAQ,EAAE,oBAAQ,CAAC,QAAQ,EAAE,oBAAQ,CAAC,QAAQ,EAAE;IAC9F,IAAI,EAAE,oBAAQ,CAAC,IAAI;IACnB,IAAI,EAAE,oBAAQ,CAAC,IAAI;IACnB,OAAO,EAAE,oBAAQ,CAAC,OAAO;IACzB,IAAI,EAAE;QACJ,GAAG,EAAE,oBAAQ,CAAC,IAAI,CAAC,GAAG;QACtB,GAAG,EAAE,oBAAQ,CAAC,IAAI,CAAC,GAAG;QACtB,OAAO,EAAE,oBAAQ,CAAC,IAAI,CAAC,OAAO;QAC9B,IAAI,EAAE,oBAAQ,CAAC,IAAI,CAAC,IAAI;KACzB;CACF,CAAC,CAAC","sourcesContent":["import { dbConfig } from '../config/db.config';\nim